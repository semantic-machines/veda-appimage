  function setSearchHelperObjToDefault(){
    searchHelperObj = {
      org:{
        handlered: 0
      },
      dep:{
        handlered: 0
      },
      app:{
        handlered: 0
      },
      phone:{
        handlered: 0
      },
      email:{
        handlered: 0
      },
      pos:{
        handlered: 0
      }
    };
